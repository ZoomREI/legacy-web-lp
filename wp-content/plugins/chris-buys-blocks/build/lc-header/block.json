{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "chris-buys/lc-header",
  "version": "0.1.0",
  "title": "LC Header",
  "category": "widgets",
  "icon": "align-center",
  "description": "Dynamic Header for the Life Changes CW Design",
  "supports": {
    "html": false
  },
  "textdomain": "chris-buys-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js",
  "attributes": {
    "selectedMarket": {
      "type": "string",
      "default": "St. Louis"
    },
    "phoneNumber": {
      "type": "string",
      "default": "(314) 887-8043"
    }
  }
}