{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "carrot-blocks/carrot-hero",
	"version": "0.1.0",
	"title": "Carrot Hero",
	"category": "widgets",
	"icon": "superhero-alt",
	"description": "A block to display the 'Carrot Hero' section",
	"supports": {
		"html": false
	},
	"textdomain": "carrot-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js",
	"attributes": {
		"selectedMarket": {
			"type": "string",
			"default": "stl"
		},
		"phoneNumber": {
			"type": "string",
			"default": "(314) 887-8043 "
		},
		"formId": {
			"type": "string",
			"default": "1"
		}
	}
}
